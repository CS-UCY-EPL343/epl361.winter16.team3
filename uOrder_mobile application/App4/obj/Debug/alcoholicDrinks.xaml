        <ListViewItem Content="Espresso" FontSize="20" Margin="22,0,0,219" VerticalAlignment="Bottom" HorizontalAlignment="Left" RenderTransformOrigin="0.547,2.607"/>
        <ListViewItem Content="Cappuccino" FontSize="20" HorizontalAlignment="Left" Margin="22,305,0,296" d:LayoutOverrides="Height, TopPosition, BottomPosition"/>
